# default mailcap for Chawan

text/gopher;	exec "$CHA_LIBEXEC_DIR"/gopher2html -u '%u'; x-htmloutput
text/gemini;	exec "$CHA_LIBEXEC_DIR"/gmi2html; x-htmloutput; x-needsstyle
text/markdown;	exec "$CHA_LIBEXEC_DIR"/md2html; x-htmloutput
text/x-ansi;	exec "$CHA_LIBEXEC_DIR"/ansi2html -st '%{title}'; x-htmloutput; x-needsstyle
text/x-dirlist;	exec "$CHA_LIBEXEC_DIR"/dirlist2html -t '%{title}'; x-htmloutput
text/uri-list;	exec "$CHA_LIBEXEC_DIR"/uri2html '%{title}'; x-htmloutput
application/xhtml+xml; exec cat; x-htmloutput
image/png;	exec "$CHA_LIBEXEC_DIR"/img2html '%t' '%u'; x-htmloutput
image/jpeg;	exec "$CHA_LIBEXEC_DIR"/img2html '%t' '%u'; x-htmloutput
image/bmp;	exec "$CHA_LIBEXEC_DIR"/img2html '%t' '%u'; x-htmloutput
image/gif;	exec "$CHA_LIBEXEC_DIR"/img2html '%t' '%u'; x-htmloutput
image/webp;	exec "$CHA_LIBEXEC_DIR"/img2html '%t' '%u'; x-htmloutput
image/svg+xml;	exec "$CHA_LIBEXEC_DIR"/img2html '%t' '%u'; x-htmloutput
